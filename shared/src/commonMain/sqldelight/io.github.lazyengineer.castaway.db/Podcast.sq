CREATE TABLE Podcast(
url TEXT NOT NULL PRIMARY KEY,
name TEXT NOT NULL
);

insertPodcast:
INSERT OR REPLACE INTO Podcast(url, name) VALUES(?,?);

selectByUrl:
SELECT *
FROM Podcast
WHERE url = ?;

selectAll:
SELECT * FROM Podcast;

deleteByUrl:
DELETE
FROM Podcast
WHERE url = ?;

deleteAll:
DELETE FROM Podcast;
